            maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
            prefWidth="800.0"
            fx:controller="tool.MainController">
    <children>
        <TabPane fx:id="tabPane" tabClosingPolicy="UNAVAILABLE"
                 AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                 AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab fx:id="movies" text="Movies"/>
                <Tab fx:id="rooms" text="Rooms"/>
                <Tab fx:id="screenings" text="Screenings"/>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
